import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int lp=sc.nextInt();
        if(lp%4==0 && lp%100!=0||lp%400==0){
            System.out.println(lp+" is a leap year");
        }
        else{
            System.out.println(lp+" is not a leap year");
        }
    }
}
