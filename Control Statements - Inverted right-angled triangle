import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int i,j;
        for(i=n;i>0;i--)
        {
            for(j=i-1;j>=0;j--)
            {
                System.out.print("*");
            }
            System.out.print("\n");
            
        }
    }
}
