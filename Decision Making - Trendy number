import java.io.*;
import java.util.*;

public class Solution {

public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    if(a>=100&&a<=999){
        int b=a/10;
    int mid=b%10;
    if(mid%3==0)

    {
        System.out.println("Trendy Number");
        }
    else
        System.out.println("Not a Trendy Number");
    }
    else
        System.out.println("Invalid Number");


}
}
